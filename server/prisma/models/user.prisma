model User {
  id         String   @id @default(uuid())
  first_name String
  last_name  String
  email      String   @unique
  phone      String
  address    String
  role       Role     @default(USER)
  created_at DateTime @default(now())

  modified_animals  Animal[]
  user_assignments  AnimalAssignment[] @relation("User")
  staff_assignments AnimalAssignment[] @relation("Staff")
  medical_logs      MedicalLog[]
}
